    "@types/moment": "^2.13.0",
    "@types/ms": "^0.7.30",
    "@types/request": "^2.48.1",
    "@types/uuid": "^3.4.4",
    "bcrypt": "^3.0.6",
    "egg": "^2.6.1",
    "egg-jwt": "^3.1.6",
    "egg-mongoose": "^3.1.1",
    "egg-redis": "^2.4.0",
    "egg-scripts": "^2.6.0",
    "egg-session": "^3.1.0",
    "egg-validate": "^2.0.2",
    "js-yaml": "^3.13.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.11",
    "md": "^0.6.0",
    "moment": "^2.24.0",
    "ms": "^2.1.2",
    "parameter": "^3.6.0",
